{
  "author": "critesjosh @critesjosh",
  "bin": {
    "boj": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^4",
    "circomlibjs": "^0.1.7",
    "dotenv": "^16.3.2",
    "viem": "^2.4.0"
  },
  "description": "Simple CLI tool to interact with the Bank of Jubjub",
  "devDependencies": {
    "boj-types": "^1.0.0",
    "boj-utils": "^1.0.0",
    "boj-coordinators": "^1.0.0",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^10",
    "oclif": "^4.0.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/bank-of-jubjub/base",
  "license": "MIT",
  "main": "",
  "name": "bojcli",
  "oclif": {
    "bin": "boj",
    "dirname": "boj",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "bank-of-jubjub/base",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/bank-of-jubjub/base/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
